{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"Kestrel": {
		"Certificates": {
			"Default": {
				"Path": "certificate.pfx",
				"Password": "12345678"
			}
		},
		"Endpoints": {
			"Http": {
				"Url": "http://*:5000"
			},
			"Https": {
				"Url": "https://*:5001",
				"Certificate": {
					"Path": "certificate.pfx",
					"KeyPassword": "12345678"
				}
			}
		},
		"Limits": {
			"MaxConcurrentConnections": 100,
			"MaxRequestBodySize": 10485760
		}
	}
}